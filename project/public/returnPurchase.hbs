<!DOCTYPE html>
<html ng-app="returnPurchase" lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <!-- Bootstrap core CSS -->
    <link href="css/bootstrap.min.css" rel="stylesheet">
    <!-- Material Design Bootstrap -->
    <!-- <link href="css/mdb.min.css" rel="stylesheet"> -->
    <!-- Your custom styles (optional) -->
    <link href="css/ClientInventoryStyle.css" rel="stylesheet">

    <!-- Angular -->
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.6/angular.min.js" type="text/javascript"></script>
    <!-- inventory page module -->
    <script src="returnPurchase.module.js" type="text/javascript"> </script>



    <title>Return Item Page</title>
    <!-- Font Awesome -->
    <script type="text/javascript">
      let mock = JSON.parse('{{{items}}}');
    </script>
</head>

<body ng-controller="returnController">
  <div ng-include="'includes/header.hbs'"></div>

  <div class="container">
    <table>
      <tr ng-repeat="items in records">
        <td>{{items.name}}</td>
        <td>{{items.serialNumber}}</td>
      </tr>
    </table>
  </div>


    <div ng-include="'includes/footer.hbs'"></div>
</body>
</html>
